  <input type="text" name="pirate[ships][][type]"><br>
  <label for="ship_1_height">Ship 1 Booty:</label>
  <input type="text" name="pirate[ships][][booty]"><br>
  <label for="ship_2_name">Ship 2 Name:</label>
  <input type="text" name="pirate[ships][][name]"><br>
  <label for="ship_2_weight">Ship 2 Type:</label>
  <input type="text" name="pirate[ships][][type]"><br>
  <label for="ship_2_height">Ship 2 Booty:</label>
